{
  "name": "sledsworth.css",
  "version": "2.0.0",
  "description": "Common CSS for resets, utilities, and default theming.",
  "main": "dist/index.css",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "sass --style=compressed ./src:./dist",
    "build:watch": "sass --watch ./src:./dist",
    "clean": "rm -rf ./dist",
    "prepublish": "npm run build:prod",
    "start": "http-server ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://sledsworth@github.com/sledsworth/sledsworth.css.git"
  },
  "keywords": [
    "css",
    "web",
    "reset"
  ],
  "author": "Steve Ledsworth",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sledsworth/sledsworth.css/issues"
  },
  "homepage": "https://github.com/sledsworth/sledsworth.css#readme",
  "devDependencies": {
    "clean-css": "^5.1.5",
    "fast-glob": "^3.2.7",
    "http-server": "^13.0.2",
    "sass": "^1.41.0"
  }
}
